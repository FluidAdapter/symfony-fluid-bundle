{
  "name": "fluid-adapter/symfony-fluid-bundle",
  "type": "symfony-bundle",
  "description": "Fluid integration for Symfony",
  "keywords": ["symfony","fluid","templating"],
  "homepage": "https://github.com/FluidAdapter/symfony-fluid-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Christian Spoo",
      "email": "cs@marketing-factory.de"
    },
    {
      "name": "Marc Neuhaus",
      "email": "marc@mia3.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "symfony/http-foundation": "^4.0",
    "symfony/http-kernel": "^4.0",
    "symfony/routing": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/form": "^4.0",
    "symfony/asset": "^4.0",
    "symfony/templating": "^4.1",
    "typo3fluid/fluid": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "FluidAdapter\\SymfonyFluidBundle\\": "src"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "FluidAdapter\\SymfonyFluidBundle\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
